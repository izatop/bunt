{
    "name": "@bunt/fs",
    "version": "0.26.0",
    "keywords": [
        "typescript"
    ],
    "author": {
        "name": "Artur Bier",
        "email": "izatop@gmail.com"
    },
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/cjs/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/esm/index.d.ts",
                "default": "./dist/esm/index.js"
            },
            "require": {
                "types": "./dist/cjs/index.d.ts",
                "default": "./dist/cjs/index.js"
            }
        }
    },
    "files": [
        "dist/",
        "LICENSE"
    ],
    "description": "FileSystem library",
    "repository": "git@github.com:izatop/bunt.git",
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@bunt/unit": "^0.26.0",
        "@bunt/util": "^0.26.0",
        "@types/minio": "^7.0.15",
        "@types/node-fetch": "^2.6.3",
        "minio": "^7.1.0",
        "node-fetch": "2"
    },
    "license": "MIT"
}
